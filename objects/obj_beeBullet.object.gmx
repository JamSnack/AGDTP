            //Direction
            var dir = sign(_enemy.x-x);
            var vdir = sign(_enemy.y-(y));
            
            //Horizontal Acceleration
            hAccel = approach(hAccel,maxAccel*dir,accelRate);
            
            //Vertical Acceleration
            vAccel = approach(vAccel,maxAccel*vdir,accelRate);
        }
    }
}

