          <argument>
            <kind>1</kind>
            <string>///Vine calculations

//vine centering and self-destruction
if !instance_exists(parent_id)
{
    instance_destroy();
}
else
{
    ox = parent_id.x;
    oy = parent_id.y;
}

//vine movement
vine_direction = point_direction(ox,oy,vine_target_x,vine_target_y);
image_angle = vine_direction;
vine_length = point_distance(ox,oy,vine_target_x,vine_target_y);
vine_parts = floor(vine_length/vine_segment_length);
vine_direction_x = abs(lengthdir_x(vine_segment_length, vine_direction));
vine_direction_y = abs(lengthdir_y(vine_segment_length, vine_direction));

print("--");
print(vine_direction_x);
print(vine_direction_y);


if reversing == false &amp;&amp; point_distance(vine_x,vine_y,vine_target_x,vine_target_y) &gt; 4 
{
    //Move the vine toward the target point
    if vine_x != vine_target_x {
        vine_x = approach(vine_x,vine_target_x,vine_direction_x);
    }
    
    if vine_y != vine_target_y {
        vine_y = approach(vine_y,vine_target_y,vine_direction_y);
    }
}
else if behavior_return == true// &amp;&amp; point_distance(vine_x,vine_y,vine_target_x,vine_target_y) &lt; 2
{
    reversing = true;
    

    
    //Bring the vine back home without affecting target_point
    if vine_x != ox {
        vine_x = approach(vine_x,ox,vine_direction_x);
    }
    
    if vine_y != oy {
        vine_y = approach(vine_y,oy,vine_direction_y);
    }
    
    if point_distance(vine_x,vine_y,ox,oy) &lt; 4{
        print("obj_vine&gt; death");
        instance_destroy();
    }
}

//Grab an instance

if instance_to_grab != noone &amp;&amp; instance_exists(instance_to_grab)
{
    if instance_grabbed == false
    {
        var _grab = collision_circle(vine_x,vine_y,4,instance_to_grab,false,true);
    
        if _grab == instance_to_grab
        {
            instance_grabbed = true;
        }
    }
    else
    {
        var _xx = vine_x;
        var _yy = vine_y;
    
        with instance_to_grab
        {
            x = _xx;
            y = _yy;
        }
    }
}

